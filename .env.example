NEXT_PUBLIC_API_BASE_URL=https://api.invotick.com/api
# Token strategy: access or access-refresh
NEXT_PUBLIC_TOKEN_STRATEGY=access
# Cookie names for auth (must match middleware/token storage)
NEXT_PUBLIC_ACCESS_COOKIE_NAME=invotick_access_token
NEXT_PUBLIC_REFRESH_COOKIE_NAME=invotick_refresh_token
# Social auth client IDs (placeholders)
NEXT_PUBLIC_GOOGLE_CLIENT_ID=your-google-client-id
NEXT_PUBLIC_FACEBOOK_CLIENT_ID=your-facebook-client-id
# Upload endpoint and limits
NEXT_PUBLIC_UPLOAD_URL=/api/uploads
NEXT_PUBLIC_UPLOAD_MAX_SIZE_MB=10
NEXT_PUBLIC_UPLOAD_ALLOWED_TYPES=image/*,application/pdf
# Locale and currency defaults (comma-separated)
NEXT_PUBLIC_LOCALES=en-US,ur-PK
NEXT_PUBLIC_CURRENCIES=USD,PKR
# Cookie names for auth (must match middleware/token storage)
NEXT_PUBLIC_ACCESS_COOKIE_NAME=invotick_access_token
NEXT_PUBLIC_REFRESH_COOKIE_NAME=invotick_refresh_token
